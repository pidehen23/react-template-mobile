      "commit-msg": "commitlint -c .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint src --ext .tsx,.ts --color --fix"
    ],
    "src/**/*.{css,less,scss}": [
      "stylelint --config .stylelintrc.json --fix"
    ],
    "src/**/*.{tsx,ts,jsx,js,css,less,md}": [
